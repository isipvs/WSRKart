<Page x:Class="WSRKart.ProofRacer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSRKart"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProofRacer" Background="White"
      ShowsNavigationUI="False"
      FontSize="18">
    <StackPanel>
        <Label Content="Спасибо за вашу ркгистрацию в качестве гонщика!" FontSize="20"
               HorizontalContentAlignment="Center" Margin="0,20,0,0"/>

        <Label Content="С вами свяжутся по поводу оплаты" HorizontalContentAlignment="Center"/>


        <Button Content="OК" HorizontalAlignment="Center" Padding="5,5,5,5" 
                x:Name="NOK" Click="OK" />
    </StackPanel>
</Page>
